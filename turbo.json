{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "prepare": {
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build", "prepare"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "lint": { },
    "fix": {
      "cache": false
    },
    "test:unit": {
      "cache": false
    },
    "coverage": {
      "cache": false
    }
  }
}
